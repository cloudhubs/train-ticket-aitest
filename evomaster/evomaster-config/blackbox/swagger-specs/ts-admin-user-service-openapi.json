{
  "openapi": "3.0.1",
  "info": {
    "title": "Train Ticket Admin User Service API",
    "description": "OpenAPI specification for Train Ticket Admin User Service",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8888/api/v1",
      "description": "Gateway Server"
    }
  ],
  "paths": {
    "/adminuserservice/users": {
      "put": {
        "tags": ["admin-user-service"],
        "summary": "Update a user",
        "operationId": "updateUser",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UserDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string"
          },
          "userName": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "gender": {
            "type": "integer",
            "format": "int32"
          },
          "documentType": {
            "type": "integer",
            "format": "int32"
          },
          "documentNum": {
            "type": "string"
          },
          "email": {
            "type": "string"
          }
        }
      },
      "Response": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "msg": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
}


