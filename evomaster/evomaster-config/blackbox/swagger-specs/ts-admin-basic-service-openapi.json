{
  "openapi": "3.0.1",
  "info": {
    "title": "Train Ticket Admin Basic Service API",
    "description": "OpenAPI specification for Train Ticket Admin Basic Service",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8888/api/v1",
      "description": "Gateway Server"
    }
  ],
  "paths": {
    "/adminbasicservice/adminbasic/prices": {
      "get": {
        "tags": ["admin-basic-service"],
        "summary": "Get all prices",
        "operationId": "getAllPrices",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["admin-basic-service"],
        "summary": "Create a new price configuration",
        "operationId": "createPrice",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PriceInfo"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Price created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          }
        }
      }
    },
    "/adminbasicservice/adminbasic/stations/{id}": {
      "delete": {
        "tags": ["admin-basic-service"],
        "summary": "Delete a station",
        "operationId": "deleteStation",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Station deleted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          },
          "404": {
            "description": "Station not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PriceInfo": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "trainType": {
            "type": "string"
          },
          "routeId": {
            "type": "string"
          },
          "basicPriceRate": {
            "type": "number",
            "format": "double"
          },
          "firstClassPriceRate": {
            "type": "number",
            "format": "double"
          }
        }
      },
      "Response": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "msg": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "bearerAuth": []
    }
  ]
}


